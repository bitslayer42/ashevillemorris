<link rel="stylesheet" href="bundle.css">

<h2 class="galleries">Photo Galleries</h2>
<div class="cards">
{% for team in collections.teams %}
    <article>
    <a href="{{ team.url }}" class="card-link"></a>
    <h2>{{ team.data.title }}</h2>
    <img src="{{ team.data.icon }}">
    <div>{{ team.data.descrip}}</div>
    </article>
{% endfor %}
</div>
